root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{png,jpg,jpeg,gif,mp4,pdf,ico,zip,gz}]
end_of_line = unset
insert_final_newline = unset
indent_style = unset
indent_size = unset
trim_trailing_whitespace = unset
