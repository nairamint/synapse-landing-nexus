# Production Environment Variables for Vercel Deployment
# These should be configured in Vercel Dashboard under Project Settings > Environment Variables

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# PostHog Configuration (Production)
VITE_POSTHOG_KEY=your_production_posthog_key
VITE_POSTHOG_HOST=https://app.posthog.com

# API Keys (Production)
VITE_OPENAI_API_KEY=your_production_openai_api_key
VITE_NEXUS_API_KEY=your_production_nexus_api_key

# Application Configuration
VITE_APP_ENV=production
VITE_APP_URL=https://your-domain.vercel.app
VITE_API_BASE_URL=https://your-api-domain.com

# Security Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Feature Flags
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_DEBUG_MODE=false