# ============================================================================
# SFDR NAVIGATOR - ENVIRONMENT CONFIGURATION
# ============================================================================

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# External API Keys
VITE_NEXUS_API_KEY=your_nexus_api_key
VITE_OCR_API_KEY=your_ocr_space_api_key

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://your-project.supabase.co/functions/v1/websocket-server

# Analytics
VITE_POSTHOG_KEY=your_posthog_key
VITE_POSTHOG_HOST=https://app.posthog.com

# External Service URLs
VITE_ESMA_API_URL=https://registers.esma.europa.eu/solr/esma_registers
VITE_NEXUS_EXTERNAL_URL=https://nexus-82zwpw7xt-aas-projects-66c93685.vercel.app

# Feature Flags
VITE_ENABLE_OCR=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_ESMA_INTEGRATION=true
VITE_ENABLE_EXTERNAL_API_FALLBACK=true

# Processing Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_OCR_CONFIDENCE_THRESHOLD=0.7
VITE_MAX_UPLOAD_RETRIES=3

# Development Configuration
VITE_DEV_MODE=false
VITE_ENABLE_MOCK_DATA=false
VITE_LOG_LEVEL=info

# Security Configuration
VITE_ENABLE_CORS=true
VITE_ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com

# Performance Configuration
VITE_API_TIMEOUT=30000
VITE_WEBSOCKET_RECONNECT_INTERVAL=5000
VITE_MAX_WEBSOCKET_RECONNECT_ATTEMPTS=5